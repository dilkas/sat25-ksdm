a = {1,...,3}

e(a, a)
cone(a)
ctwo(a)
cthree(a)
cfour(a)

!e(x, x).
e(x, y) => e(y, x).
cone(x) v ctwo(x) v cthree(x) v cfour(x).
!cone(x) v !ctwo(x).
!cone(x) v !cthree(x).
!cone(x) v !cfour(x).
!ctwo(x) v !cthree(x).
!ctwo(x) v !cfour(x).
!cthree(x) v !cfour(x).
e(x, y) => ((cone(x) ^ !cone(y)) v (ctwo(x) ^ !ctwo(y)) v (cthree(x) ^ !cthree(y)) v (cfour(x) ^ !cfour(y))).
